{
  "manifest_version": 2,
  "name": "Simple Reload",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "simple_off.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
